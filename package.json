{
  "name": "mega",
  "version": "5.3.0",
  "description": "High performance multi-device whatsapp bot",
  "type": "commonjs",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "data": "data",
    "plugins": "plugins"
  },
  "scripts": {
    "start": "node index.js",
    "lint": "node test.js",
    "start:optimized": "node --max-old-space-size=512 --optimize-for-size --gc-interval=100 index.js",
    "cleanup": "node cleanup.js",
    "start:clean": "npm run cleanup && npm run start:optimized",
    "install:panel": "npm install --legacy-peer-deps"
  },
  "keywords": [ "termux-whatsapp-bot", "whatsapp-bot", "whatsapp-api",
